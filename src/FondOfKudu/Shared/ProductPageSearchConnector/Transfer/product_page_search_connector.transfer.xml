<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="SortConfig">
        <property name="name" type="string"/>
        <property name="parameterName" type="string"/>
        <property name="fieldName" type="string"/>
        <property name="isDescending" type="bool"/>
    </transfer>

    <transfer name="PageMap">
        <property name="available" type="int"/>
        <property name="isSoldOut" type="string"/>
    </transfer>

    <transfer name="Store" />

    <transfer name="ProductAbstractAvailability">
        <property name="isNeverOutOfStock" type="bool"/>
        <property name="availability" type="decimal"/>
    </transfer>
</transfers>
